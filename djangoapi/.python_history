from django.db import connection
cursor = connection.cursor()
print("Conexi√≥n exitosa")
cursor.execute("""
    SELECT table_name 
    FROM information_schema.tables 
    WHERE table_schema = 'eventos'
    ORDER BY table_name;
""")
tables = cursor.fetchall()
print("\nTablas en el esquema 'eventos':")
for table in tables:
    print(f"  - {table[0]}")
exit()
